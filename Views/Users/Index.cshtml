@model IEnumerable<SafetyPatrol.Models.tbl_users>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            empno
        </th>
        <th>
            emplname
        </th>
        <th>
            empfname
        </th>
        <th>
            empmname
        </th>
        <th>
            password
        </th>
        <th>
            email
        </th>
        <th>
            userlevel
        </th>
        <th>
            aka
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.empno)
