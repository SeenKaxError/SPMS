            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.userlevel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.aka)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.recno }) |
